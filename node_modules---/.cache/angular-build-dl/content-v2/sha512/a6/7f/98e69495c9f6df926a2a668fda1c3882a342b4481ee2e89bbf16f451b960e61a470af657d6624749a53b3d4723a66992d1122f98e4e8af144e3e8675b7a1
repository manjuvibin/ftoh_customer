    var ChangePasswordPage = /*#__PURE__*/function () {
      function ChangePasswordPage(config, shared, loading) {
        _classCallCheck(this, ChangePasswordPage);

        this.config = config;
        this.shared = shared;
        this.loading = loading;
        this.myAccountData = {
          confirmPassword: '',
          newpassword: '',
          oldpassword: '',
          customers_id: ''
        };
      } //============================================================================================  
      //function updating user information


      _createClass(ChangePasswordPage, [{
        key: "updatePassword",
        value: function updatePassword() {
          var _this = this;

          if (this.myAccountData.confirmPassword != this.myAccountData.newpassword) {
            console.log(this.myAccountData.confirmPassword + "  " + this.myAccountData.newpassword);
            this.shared.toast("Confirm Password is Wrong!");
            return;
          }

          this.loading.show();
          var url = 'updatepassword?oldpassword=' + this.myAccountData.oldpassword + "&newpassword=" + this.myAccountData.newpassword + "&customers_id=" + this.shared.customerData.customers_id;
          this.config.getHttp(url).then(function (data) {
            _this.loading.hide();

            if (data.success == 1) {
              var d = data.data[0];

              _this.shared.toast(data.message);

              _this.myAccountData.oldpassword = "";
              _this.myAccountData.confirmPassword = "";
              _this.myAccountData.newpassword = "";
            } else {
              _this.shared.toast(data.message);
            }
          }, function (error) {
            _this.loading.hide();

            _this.shared.toast("Error while Updating!");
          }); //}
        }
      }, {
        key: "ngOnInit",
        value: function ngOnInit() {}
      }]);

      return ChangePasswordPage;
